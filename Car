// Superclass
open class Vehicle {
  var carBrand = ""
  var topSpeed = 0
  var maxSeat = 0
}

// Subclass
class Car: Vehicle() {
  fun showCar() {
    println(Car.carBrand) // x is defined in the superclass
  }
}

// Create an object of the MyChildClass and call myFunction
fun main() {
  val myObj = Car()
  Car.carBrand = "Nissan"
  Car.showCar()
}